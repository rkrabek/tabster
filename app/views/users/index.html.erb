<h1>Users</h1>
<%= link_to 'Create New User', new_user_path %>
<ul> <% @users.each do |user| %>
	<li><%= link_to user.name, user_path(user) %> | 
		<%= link_to 'Edit', edit_user_path(user) %> | 
		<%= link_to 'Destroy', user_path(user),
		method: :delete,
		data: {confirm: 'Are you sure?'} %></li>
	<% end %>
</ul>